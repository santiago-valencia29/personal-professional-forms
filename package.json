{
  "name": "personal-professional-forms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start-pwa": "ng build --configuration production --aot --base-href=/ && http-server -p 8080 -c-1 dist/personal-professional-forms",
    "deploy": "ng build --configuration production --aot && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/cdk": "17.3.10",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/material": "17.3.10",
    "@angular/material-moment-adapter": "^17.3.10",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@angular/service-worker": "17.3.12",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ngxs/devtools-plugin": "^18.0.0",
    "@ngxs/logger-plugin": "^18.0.0",
    "@ngxs/store": "^18.0.0",
    "@popperjs/core": "^2.11.6",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "bootstrap": "^5.2.3",
    "http-server": "^14.1.1",
    "jest-preset-angular": "^14.4.2",
    "rxjs": "~7.4.0",
    "subsink": "^1.0.2",
    "sweetalert2": "^11.7.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.11",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular/cli": "17.3.0",
    "@angular/compiler-cli": "17.3.12",
    "@angular/localize": "17.3.12",
    "@types/aos": "^3.0.4",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "angular-cli-ghpages": "^2.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.3.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/config.jest.ts"
    ],
    "moduleNameMapper": {
      "@app/(.)": "<rootDir>/src/app/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!rxjs|@angular)"
    ]
  }
}
